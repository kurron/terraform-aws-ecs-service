[{
    "family": "Nginx",
    "containerDefinitions": [
        {
            "name": "first",
            "image": "nginx:latest",
            "cpu": 256,
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "FOO",
                    "value": "bar"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-region": "us-west-2",
                    "awslogs-group": "terra",
                    "awslogs-create-group": "true"
                }
            }
        }
    ]
}]
