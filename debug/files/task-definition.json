[
    {
        "name": "Nginx",
        "image": "nginx:latest",
        "cpu": 256,
        "memory": 128,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 0,
                "protocol": "tcp"
            }
        ],
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "us-west-2",
                "awslogs-group": "terra"
            }
        }
    }
]
